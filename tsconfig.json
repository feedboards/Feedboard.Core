{
    "compilerOptions": {
        "module": "ES6",
        "target": "ESNext",
        "declaration": true,
        "outDir": "./dist",
        "moduleResolution": "node",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules", "dist"],
    "include": ["src"]
}
